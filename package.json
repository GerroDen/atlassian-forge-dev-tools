{
  "name": "atlassian-forge-dev-tools",
  "version": "0.1.1",
  "packageManager": "yarn@4.6.0",
  "type": "module",
  "scripts": {
    "dev": "vite build --mode development",
    "build": "run-s 'build:*'",
    "build:vite": "vite build",
    "build:web-ext-lint": "web-ext lint -s dist/ --pretty",
    "lint": "run-p 'lint:*'",
    "lint:type": "vue-tsc --noEmit",
    "lint:oxlint": "oxlint --quiet",
    "prepare": "husky"
  },
  "dependencies": {
    "chrome-devtools-frontend": "^1.0.1418433",
    "lodash-es": "^4.17.21",
    "vue": "^3.5.13",
    "vue-json-pretty": "^2.4.0",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/chrome": "^0.0.307",
    "@types/lodash-es": "^4.17.12",
    "@types/webextension-polyfill": "^0.12.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "npm-run-all": "^4.1.5",
    "oxlint": "^0.15.12",
    "prettier": "^3.5.2",
    "prettier-plugin-css-order": "^2.1.2",
    "typescript": "~5.7.3",
    "unocss": "^66.0.0",
    "vite": "^6.2.0",
    "vite-plugin-checker": "^0.9.0",
    "vite-plugin-oxlint": "^1.2.3",
    "vite-plugin-web-extension": "^4.4.3",
    "vue-tsc": "^2.2.4",
    "web-ext": "^8.4.0"
  }
}
